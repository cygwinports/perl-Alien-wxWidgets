  /usr/lib/perl5/vendor_perl/5.8/cygwin/Alien/wxWidgets/Config/motif_2_8_4_gcc_3_4.pm
  /usr/lib/perl5/vendor_perl/5.8/cygwin/Alien/wxWidgets/Config/x11_2_6_4_gcc_3_4.pm
  /usr/lib/perl5/vendor_perl/5.8/cygwin/Alien/wxWidgets/Config/x11_2_8_4_gcc_3_4.pm
